{
  "card": {
    "log_id": "preferred_partners_sdui",
    "states": [
      {
        "state_id": 0,
        "div": {
          "type": "container",
          "items": [
            {
              "type": "text",
              "text": "@{title}",
              "font_size": 18,
              "font_weight": "medium",
              "text_color": "#17181c",
              "width": {
                "type": "match_parent"
              },
              "height": {
                "type": "wrap_content"
              },
              "margins": {
                "right": 15,
                "left": 15,
                "bottom": 10
              }
            },
            {
              "type": "gallery",
              "id": "ad_gallery",
              "width": {
                "type": "match_parent"
              },
              "height": {
                "type": "wrap_content"
              },
              "item_builder": {
                "data": "@{item_data}",
                "data_element_name": "item",
                "prototypes": [
                  {
                    "div": {
                      "type": "gallery_item_template",
                      "descriptions": "@{item.getArray('descriptions')}"
                    }
                  }
                ]
              },
              "paddings": {
                "right": 15,
                "left": 15
              }
            }
          ],
          "height": {
            "type": "wrap_content"
          },
          "width": {
            "type": "match_parent"
          },
          "orientation": "vertical",
          "content_alignment_horizontal": "left",
          "content_alignment_vertical": "top"
        }
      }
    ],
    "variables": [
      {
        "type": "array",
        "name": "item_data",
        "value": [
          {
            "image_url": "https://figma-alpha-api.s3.us-west-2.amazonaws.com/images/a622e02e-4d9a-4158-b4e6-21e29eb91f6a",
            "name": "{{response.offers.title}}",
            "discount": "{{response.offers.title}}",
            "discount_summary": "{{response.offers.title}}",
            "discount_code": "{{response.offers.title}}",
            "card_action_url": "{{response.offers.title}}",
            "log_id": "{{response.offers.title}}",
            "new_key": "{{response.offers.title}}",
            "discount_btn_action_url": "{{response.offers.title}}",
            "descriptions": "{{response.offers.descriptions}}"
          }
        ]
      },
      {
        "type": "string",
        "name": "title",
        "value": "Preferred Partners"
      }
    ]
  },
  "templates": {
    "gallery_item_template": {
      "type": "container",
      "background": [
        {
          "type": "solid",
          "color": "#ffffff"
        }
      ],
      "border": {
        "stroke": {
          "color": "#e4e4e7",
          "width": 1
        },
        "corner_radius": 15
      },
      "items": [
        {
          "type": "image",
          "image_url": "@{item.getString('image_url')}",
          "height": {
            "type": "fixed",
            "value": 135
          },
          "width": {
            "type": "match_parent"
          },
          "scale": "fill"
        },
        {
          "type": "container",
          "items": [
            {
              "type": "text",
              "text": "@{item.getString('name')}",
              "font_size": 18,
              "font_weight": "bold",
              "line_height": 25,
              "text_color": "#17181c",
              "width": {
                "type": "match_parent"
              },
              "height": {
                "type": "wrap_content"
              }
            },
            {
              "type": "separator",
              "width": {
                "type": "match_parent"
              },
              "height": {
                "type": "fixed",
                "value": 1
              },
              "delimiter_style": {
                "color": "#c8cad0"
              },
              "margins": {
                "top": 5,
                "bottom": 5
              }
            },
            {
              "type": "container",
              "item_builder": {
                "$data": "descriptions",
                "data_element_name": "child_item",
                "prototypes": [
                  {
                    "div": {
                      "type": "container",
                      "orientation": "horizontal",
                      "items": [
                        {
                          "type": "text",
                          "text": "• ",
                          "text_color": "#5e616e",
                          "font_size": "12",
                          "line_height": 17,
                          "width": {
                            "type": "wrap_content"
                          },
                          "height": {
                            "type": "wrap_content"
                          }
                        },
                        {
                          "type": "text",
                          "text": "@{child_item.getString('text')}",
                          "text_color": "#5e616e",
                          "font_size": "12",
                          "line_height": 17,
                          "width": {
                            "type": "match_parent"
                          },
                          "height": {
                            "type": "wrap_content"
                          }
                        }
                      ],
                      "margins": {
                        "bottom": 2
                      }
                    }
                  }
                ]
              },
              "height": {
                "type": "wrap_content"
              },
              "width": {
                "type": "match_parent"
              },
              "orientation": "vertical",
              "margins": {
                "top": 5
              }
            }
          ],
          "margins": {
            "top": 15,
            "right": 15,
            "bottom": 15,
            "left": 15
          }
        },
        {
          "type": "container",
          "items": [
            {
              "type": "container",
              "items": [
                {
                  "type": "text",
                  "text": "@{item.getString('discount')}",
                  "font_size": 16,
                  "font_weight": "bold",
                  "line_height": 22,
                  "text_color": "#17181c",
                  "width": {
                    "type": "match_parent"
                  },
                  "height": {
                    "type": "wrap_content"
                  }
                },
                {
                  "type": "text",
                  "text": "@{item.getString('discount_summary')}",
                  "font_size": 10,
                  "font_weight": "regular",
                  "text_color": "#17181c",
                  "width": {
                    "type": "match_parent"
                  },
                  "height": {
                    "type": "wrap_content"
                  }
                }
              ],
              "height": {
                "type": "wrap_content"
              },
              "width": {
                "type": "match_parent",
                "weight": 6
              },
              "orientation": "vertical"
            },
            {
              "type": "container",
              "items": [
                {
                  "type": "container",
                  "items": [
                    {
                      "type": "image",
                      "image_url": "https://figma-alpha-api.s3.us-west-2.amazonaws.com/images/eaf54dfa-cc72-4468-a288-5db8ecf9cbb6",
                      "height": {
                        "type": "fixed",
                        "value": 16
                      },
                      "width": {
                        "type": "fixed",
                        "value": 14
                      }
                    }
                  ],
                  "height": {
                    "type": "fixed",
                    "value": 24
                  },
                  "width": {
                    "type": "fixed",
                    "value": 24
                  },
                  "orientation": "overlap",
                  "content_alignment_vertical": "center",
                  "content_alignment_horizontal": "center"
                },
                {
                  "type": "text",
                  "text": "@{item.getString('discount_code')}",
                  "font_size": 16,
                  "font_weight": "bold",
                  "line_height": 22,
                  "text_color": "#fafafa",
                  "margins": {
                    "left": 5
                  },
                  "width": {
                    "type": "match_parent"
                  },
                  "height": {
                    "type": "wrap_content"
                  }
                }
              ],
              "background": [
                {
                  "type": "solid",
                  "color": "#17181c"
                }
              ],
              "border": {
                "corners_radius": {
                  "top-left": 5,
                  "top-right": 5,
                  "bottom-left": 5,
                  "bottom-right": 5
                }
              },
              "height": {
                "type": "wrap_content"
              },
              "width": {
                "type": "match_parent",
                "weight": 4
              },
              "orientation": "horizontal",
              "paddings": {
                "top": 2,
                "bottom": 2,
                "left": 5,
                "right": 5
              },
              "margins": {
                "left": 10
              },
              "content_alignment_vertical": "center",
              "content_alignment_horizontal": "center"
            }
          ],
          "background": [
            {
              "type": "solid",
              "color": "#f4f5f5"
            }
          ],
          "border": {
            "corners_radius": {
              "top-left": 10,
              "top-right": 10,
              "bottom-left": 10,
              "bottom-right": 10
            },
            "stroke": {
              "color": "#e4e4e7",
              "width": 1
            }
          },
          "paddings": {
            "top": 6,
            "right": 15,
            "bottom": 6,
            "left": 15
          },
          "margins": {
            "right": 15,
            "bottom": 15,
            "left": 15
          },
          "orientation": "horizontal",
          "actions": [
            {
              "log_id": "@{item.getString('log_id')}",
              "url": "@{item.getString('discount_btn_action_url')}"
            }
          ],
          "content_alignment_vertical": "center",
          "content_alignment_horizontal": "center"
        }
      ],
      "height": {
        "type": "wrap_content"
      },
      "width": {
        "type": "fixed",
        "value": 300
      },
      "orientation": "vertical",
      "actions": [
        {
          "log_id": "@{item.getString('log_id')}",
          "url": "@{item.getString('card_action_url')}"
        }
      ]
    },
    "_template_list_item": {
      "type": "container",
      "orientation": "horizontal",
      "items": [
        {
          "type": "text",
          "text": "• ",
          "$text_color": "list_color",
          "$font_size": "list_text_size",
          "line_height": 17,
          "width": {
            "type": "wrap_content"
          },
          "height": {
            "type": "wrap_content"
          }
        },
        {
          "type": "text",
          "$text": "list_text",
          "$text_color": "list_color",
          "$font_size": "list_text_size",
          "line_height": 17,
          "width": {
            "type": "match_parent"
          },
          "height": {
            "type": "wrap_content"
          }
        }
      ],
      "margins": {
        "bottom": 2
      }
    }
  }
}